<h1 class="User List"> </h1>

<div>
	<table>
		<tr>
			<th></th><th>Name</th><th>Email</th><th>URL:</th>
		</tr>
		<%@users.each do |user|%>
			<tr>
				<td><%= gravatar_for user, :size => 30 %> </td>
				<td> <%= user.name%></td>
				<td> <%= user.email%></td>
				<td> <%= link_to user_path(user), user %></td>
			</tr>
		<%end%>
	</table>
</div>